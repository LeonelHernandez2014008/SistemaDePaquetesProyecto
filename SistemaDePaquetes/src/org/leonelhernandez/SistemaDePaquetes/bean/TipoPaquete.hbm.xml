<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.leonelhernandez.SistemaDePaquetes.bean.TipoPaquete" table="TipoPaquete">
	    <id name="idTipoPaquete" type="java.lang.Integer">
            <column name="idTipoPaquete" />
            <generator class="sequence-identity" >
            	<param name="sequence">TIPOPAQUETE_SEQ</param>
            </generator>
        </id>
        <property name="tipoPaquete" type="java.lang.String">
            <column name="tipoPaquete" />
        </property>
        <property name="costoTipoPaquete" type="java.lang.String">
            <column name="costoTipoPaquete" />
        </property>
        
	</class>
</hibernate-mapping>